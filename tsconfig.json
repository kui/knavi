{
  "compilerOptions": {
    "skipLibCheck": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable", "WebWorker"],
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "Node16",
    "moduleResolution": "Node16",
    "allowImportingTsExtensions": true,
    "noEmit": true
  }
}
